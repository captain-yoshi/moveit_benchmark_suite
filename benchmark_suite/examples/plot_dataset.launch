<?xml version="1.0" ?>
<launch>

  <!-- Output file -->
  <arg name="files"/>

  <!-- By default, we are not in debug mode -->
  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value="" />
  <arg     if="$(arg debug)" name="launch_prefix" value="gdb --args" />

  <node name="plot_dataset" pkg="moveit_benchmark_suite" type="plot_dataset" respawn="false" output="screen" launch-prefix="$(arg launch_prefix)">
    <param name="files" type="yaml" value="$(arg files)"/>
  </node>

</launch>
